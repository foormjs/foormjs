{
  "name": "@foormjs/ui",
  "version": "0.0.2-alpha.5",
  "description": "@foormjs/ui",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "vite --force",
    "build": "vite build && vue-tsc ./src/index.ts --declaration --emitDeclarationOnly --outDir dist/ui/src",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "customBuild": true,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "default": "./dist/style.css"
    },
    "./form-vars.css": {
      "default": "./src/oo-form-vars.css"
    }
  },
  "files": [
    "dist",
    "src/oo-form-vars.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foormjs/foormjs.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "foorm",
    "foormjs",
    "forms",
    "validations"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foormjs/foormjs/issues"
  },
  "homepage": "https://github.com/foormjs/foormjs/tree/main/packages/ui#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all2": "^6.0.6",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue": "^3.3.9",
    "vue-tsc": "^1.8.11",
    "vite-plugin-dts": "^3.6.3"
  },
  "dependencies": {
    "foorm": "0.0.2-alpha.5"
  }
}
