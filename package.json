{
  "name": "foormjs",
  "version": "0.1.0",
  "description": "Backend-driven dynamic forms with dual-end validation for seamless data collection",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap",
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts packages/**/src/**/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "vitepress dev docs",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foormjs/foormjs.git"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "keywords": [
    "forms",
    "validations",
    "foorm"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foormjs/foormjs/issues"
  },
  "homepage": "https://github.com/foormjs/foormjs#readme",
