    "@types/prompts": "^2.4.4",
    "@types/swagger-ui-dist": "^3.30.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
